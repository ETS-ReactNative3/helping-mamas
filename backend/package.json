{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend for bog-vms",
  "main": "index.js",
  "scripts": {
    "start": "node ./src/bin/www",
    "dev": "dotenv nodemon ./src/bin/www --watch src",
    "format": "prettier --write '{src,test}/**/*.{js,jsx,json,css}'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "secrets": "npm run secrets:logout && cross-env-shell BW_SESSION=`bw login product@bitsofgood.org --raw` \"npm run secrets:sync\"",
    "secrets:logout": "(bw logout || exit 0)",
    "secrets:login": "bw login product@bitsofgood.org",
    "secrets:sync": "bw sync && bw get item b8594f56-00eb-4bbd-a446-ad9c0149ecc2 | fx .notes > \".env\""
  },
  "dependencies": {
    "connect-mongo": "^3.0.0",
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "express-request-id": "^1.4.1",
    "express-session": "^1.16.2",
    "express-validator": "^6.2.0",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-google-token": "^0.1.2"
  },
  "devDependencies": {
    "dotenv-cli": "^2.0.1",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2"
  }
}
