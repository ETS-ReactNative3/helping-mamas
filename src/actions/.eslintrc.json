{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": ["error"],
        "linebreak-style": 0,
        "no-underscore-dangle": "off",
        "import/prefer-default-export": "off"
    }
}
